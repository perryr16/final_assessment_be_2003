<h1>New Doctor Hire for <%=@hospital.name %></h1>

<section id="new-doctor-form">
  <%= form_for [@hospital, @doctor] do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %> 
    <br>
  
    <%= f.label :specialty %>
    <%= f.text_field :specialty %> 
    <br>
  
    <%= f.label :education %>
    <%= f.text_field :education %> 
    <br>
  
    <%= f.submit %>
  <% end %>

</section>